#!/bin/sh
#######################################################################
#### Calls the scripts for gtranslator
#######################################################################
if [ $# -lt 1 ] ; then
	exit 1
fi
#######################################################################
_scripts_dir=@SCRIPTS_DIR@
#######################################################################
$_scripts_dir/cut_the_header.sh $1
$_scripts_dir/split_it_with_awk.sh $1
exit 0
