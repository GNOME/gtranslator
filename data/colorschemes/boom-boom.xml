<?xml version="1.0"?>
<colorscheme name="boom-boom" version="0.1">
<!-- Somehow quite different color scheme for your eyes .-) -->
	<author name="Fatih Demir" email="kabalak@gtranslator.org"/>
	<color target="fg">ivory2</color>
	<color target="bg">grey30</color>
	<color target="special_char">ivory2</color>
