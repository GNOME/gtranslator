<?xml version="1.0"?>
<colorscheme name="menesis-default" version="0.1">
<!-- Gediminas' previously default colors for the syntax highlighting -->
	<author name="Gediminas Paulauskas" email="menesis@gtranslator.org"/>
	<color target="fg">white</color>
	<color target="bg">black</color>
	<color target="special_char">grey</color>
	<color target="hotkey">blue</color>
	<color target="c_format">red</color>
	<color target="number">orange</color>
	<color target="punctuation">wheat</color>
	<color target="special">maroon</color>
</colorscheme>
