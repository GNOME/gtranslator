<?xml version="1.0"?>
<colorscheme name="nizuu" version="0.2">
<!-- A newly color scheme with grey default color (nice for me) -->
	<author name="Fatih Demir" email="kabalak@gtranslator.org"/>
	<color target="fg">grey40</color>
	<color target="bg">white</color>
	<color target="special_char">grey40</color>
	<color target="hotkey">blue</color>
	<color target="c_format">maroon</color>
	<color target="number">maroon</color>
	<color target="punctuation">maroon</color>
	<color target="special">darkred</color>
	<color target="address">blue</color>
	<color target="keyword">darkblue</color>
	<color target="spell_error">red</color>
</colorscheme>
