<?xml version="1.0"?>
<colorscheme name="SpiffE" version="0.3">
  <author name="Fatih Demir" email="kabalak@gtranslator.org"/>
  <color target="fg">white</color>
  <color target="bg">lightgrey</color>
  <color target="text_bg">slate grey</color>
  <color target="special_char">grey</color>
  <color target="hotkey">light cyan</color>
  <color target="c_format">yellow</color>
  <color target="number">yellow</color>
  <color target="punctuation">lightpink</color>
  <color target="special">lightpink</color>
  <color target="address">lightpink</color>
  <color target="keyword">lightpink</color>
  <color target="spell_error">darkred</color>
</colorscheme>
