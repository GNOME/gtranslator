<?xml version="1.0"?>
<colorscheme name="Clean" version="0.2">
	<author name="Fatih Demir" email="kabalak@gtranslator.org"/>
	<color target="fg">darkblue</color>
	<color target="bg">white</color>
	<color target="text_bg">grey90</color>
	<color target="special_char">grey30</color>
	<color target="hotkey">grey30</color>
	<color target="c_format">maroon</color>
	<color target="number">darkblue</color>
	<color target="punctuation">darkblue</color>
	<color target="special">darkblue</color>
	<color target="address">darkblue</color>
	<color target="keyword">maroon</color>
	<color target="spell_error">red</color>
</colorscheme>
