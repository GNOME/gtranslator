<?xml version="1.0"?>
<colorscheme name="waliays" version="0.2">
<!-- Why this colorscheme? "We All Live In A Yellow Submarine..." -->
	<author name="Fatih Demir" email="kabalak@gtranslator.org"/>
	<color target="fg">blue</color>
	<color target="bg">yellow</color>
	<color target="text_bg">yellow3</color>
	<color target="special_char">black</color>
	<color target="hotkey">red</color>
	<color target="c_format">darkgreen</color>
	<color target="number">red</color>
	<color target="punctuation">blue</color>
	<color target="special">blue</color>
	<color target="address">darkred</color>
	<color target="keyword">darkred</color>
	<color target="spell_error">maroon</color>
</colorscheme>
