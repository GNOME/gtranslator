<?xml version="1.0"?>
<colorscheme name="default" version="0.4">
	<author name="Fatih Demir" email="kabalak@kabalak.net"/>
	<color target="fg">black</color>
	<color target="bg">white</color>
	<color target="text_bg">white</color>
	<color target="special_char">grey</color>
	<color target="hotkey">blue</color>
	<color target="c_format">red</color>
