<?xml version="1.0"?>
<colorscheme name="kbabel" version="0.4">
<!-- A KBabel colors alike scheme -->
	<author name="Fatih Demir" email="kabalak@kabalak.net"/>
	<color target="fg">black</color>
	<color target="bg">white</color>
	<color target="text_bg">LemonChiffon1</color>
	<color target="special_char">black</color>
	<color target="hotkey">maroon</color>
	<color target="c_format">blue</color>
	<color target="number">green</color>
	<color target="punctuation">darkgreen</color>
	<color target="special">green</color>
	<color target="address">darkgreen</color>
	<color target="keyword">darkgreen</color>
	<color target="spell_error">red</color>
</colorscheme>
