<?xml version="1.0"?>
<colorscheme name="kbabel" version="0.1">
<!-- A KBabel colors alike scheme -->
	<author name="Fatih Demir" email="kabalak@gtranslator.org"/>
	<color target="fg">darkgreen</color>
	<color target="bg">white</color>
	<color target="special_char">darkgreen</color>
	<color target="hotkey">green</color>
	<color target="c_format">red</color>
	<color target="number">green</color>
	<color target="punctuation">darkgreen</color>
	<color target="special">darkgreen</color>
	<color target="address">green</color>
	<color target="keyword">green</color>
</colorscheme>
