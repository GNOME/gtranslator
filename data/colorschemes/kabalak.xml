<?xml version="1.0"?>
<colorscheme name="kabalak" version="0.3">
<!-- All tags are _mandatory_; the _author_ tag MUST be before the rest! -->
	<author name="Fatih Demir" email="kabalak@gtranslator.org"/>
	<color target="fg">navy</color>
	<color target="bg">white</color>
	<color target="special_char">grey20</color>
	<color target="hotkey">blue</color>
	<color target="c_format">darkred</color>
	<color target="number">orange</color>
	<color target="punctuation">maroon</color>
	<color target="special">red</color>
	<color target="address">darkred</color>
	<color target="keyword">darkblue</color>
	<color target="spell_error">red</color>
</colorscheme>
