<?xml version="1.0"?>
<colorscheme name="vim" version="0.1">
<!-- A vim's po file syntax colors alike colorscheme. -->
	<author name="Fatih Demir" email="kabalak@gtranslator.org"/>
	<color target="fg">indianred</color>
	<color target="bg">white</color>
	<color target="special_char">indianred</color>
	<color target="hotkey">navy</color>
	<color target="c_format">red</color>
	<color target="number">red</color>
	<color target="punctuation">indianred</color>
	<color target="special">indianred</color>
	<color target="address">navy</color>
	<color target="keyword">navy</color>
	<color target="spell_error">red</color>
</colorscheme>
