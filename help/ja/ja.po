msgid "Translation Memory Tab"
msgstr "翻訳メモリ タブ"

#: C/gtranslator.xml:1395(para)
msgid "The <guilabel>Translation Memory</guilabel> tab contains:"
msgstr "<guilabel>翻訳メモリ</guilabel> タブには、以下が含まれます:"

