msgid "Forward"
msgstr ""

#: C/gtranslator.xml:523(para)
msgid "Jumps to the next message."
msgstr ""

#: C/gtranslator.xml:527(guiicon)
msgid "Back"
msgstr ""

#: C/gtranslator.xml:529(para)
msgid "Jumps to the previous message."
msgstr ""

