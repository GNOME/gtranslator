#: ../C/gtranslator.xml:1214(guilabel)
msgid "Add button"
msgstr "Hinzufügen-Knopf"

#: ../C/gtranslator.xml:1216(para)
msgid ""
"Shows the <guilabel>Profile</guilabel> dialog in order to create a new "
"profile."
msgstr ""
"Öffnet den Dialog <guilabel>Profile</guilabel> zur Erstellung eines neuen "
"Profils."

#: ../C/gtranslator.xml:1223(guilabel)
msgid "Edit button"
msgstr "Bearbeiten-Knopf"

#: ../C/gtranslator.xml:1225(para)
msgid ""
"Shows the <guilabel>Profile</guilabel> dialog in order to edit an exists "
"profile."
msgstr ""
"Öffnet den Dialog <guilabel>Profile</guilabel> zur Bearbeitung eines "
"bestehenden Profils."

#: ../C/gtranslator.xml:1232(guilabel)
msgid "Delete button"
msgstr "Löschen-Knopf"

#: ../C/gtranslator.xml:1234(para)
msgid "Removes the selected profile."
msgstr "Entfernt das ausgewählte Profil."

