<?xml version="1.0" standalone="no"?>
	<omf>
	<!-- gtranslator's OMF file for scrollkeeper -->
		<resource>
			<!-- Sidebar title in Nautilus -->
			<title>gtranslator documentation</title>
			<!-- Personal informations -->
			<creator>
				<person>
					<firstName>Emese</firstName>
					<lastName>Kovacs</lastName>
					<email>emese@gnome.hu</email>
				</person>
			</creator>
			<!-- Dept. of help .-) -->
			<subject>
				<category>Development|Development Tools|Other</category>
				<category>GNOME|Development</category>
			</subject>
			<!-- We're working with sgml files, 
				so make this known to scrollkeeper -->
			<format mime="text/sgml"/>
			<!-- The file itself. -->
			<identifier url="gtranslator.sgml"/>
			<!-- A small description -->
			<description>
			gtranslator's online documentation.
			</description>
			<!-- And at last, we'd to define our language code -->
			<language code="C"/>
		</resource>
	</omf>
