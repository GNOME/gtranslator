	<varlistentry>
	  <term><guimenu>Edit</guimenu></term>
	  <listitem>
	    <para>
	   This menu contains:
	   <itemizedlist>
                <listitem>
		  <para>
	            <menuchoice>
		      <shortcut>
			<keycombo><keycap>Ctrl</keycap><keycap>Z</keycap></keycombo>
		      </shortcut>
		      <guimenuitem>Undo</guimenuitem>
		    </menuchoice>
