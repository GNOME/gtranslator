<?xml version="1.0"?>
<!-- 
This is an example file for gtranslator's message db
	xml-format.
-->
<db language="Your language code" author="Your name" email="Your EMail">
<!-- the message db serial ( like in named's conf-files ) -->
<serial date="2000-08-01">1</serial>
	<!-- the msgid tag with his/her attributes/the msgstrs -->
	<msgid name="The sample msgid">
		<msgstr>The sample translation</msgstr>
		<msgstr>Another possbility</msgstr>
	</msgid>
	<msgid name="The 2nd sample msgid">
		<msgstr>The 2nd translation</msgstr>
	</msgid>
</db>
